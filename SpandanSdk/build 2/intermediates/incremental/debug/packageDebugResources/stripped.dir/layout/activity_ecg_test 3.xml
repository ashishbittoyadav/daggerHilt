        <in.sunfox.spandanecg.spandansdk.PixelGridCustomView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:gridLeadDirection="Vertical"
            app:gridPrimaryColor="@color/colorSevenLeadLineChartGridLine"
            app:gridSecondaryColor="@color/colorSevenLeadLineChartGridLine"
            app:gridStyle="gridDouble"
            app:layout_constraintBottom_toBottomOf="@id/activity_ecg_test_fragment_container_view"
            app:layout_constraintEnd_toEndOf="@id/activity_ecg_test_fragment_container_view"
            app:layout_constraintStart_toStartOf="@id/activity_ecg_test_fragment_container_view"
            app:layout_constraintTop_toTopOf="@id/activity_ecg_test_fragment_container_view"
            app:numPrimaryGrids="15" />

        <ImageView
            android:id="@+id/activity_ecg_test_ecg_position_image_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@id/activity_ecg_test_fragment_container_view"
            app:layout_constraintTop_toTopOf="@id/activity_ecg_test_fragment_container_view"
            app:layout_constraintEnd_toEndOf="@id/activity_ecg_test_fragment_container_view"
            app:layout_constraintBottom_toBottomOf="@id/activity_ecg_test_fragment_container_view"/>

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/activity_ecg_test_fragment_container_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginVertical="8dp"
            android:layout_marginTop="@dimen/dimenLeadCheckImageViewSize16dp"
            app:layout_constraintBottom_toTopOf="@id/activity_ecg_test_start_test_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/activity_ecg_test_ecg_title_text_view" />

        <TextView
            android:id="@+id/activity_ecg_test_start_test_button"
            style="@style/CustomButtonPrimaryLiveEcg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:text="@string/start_ecg_test"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/activity_ecg_test_generate_report_button"
            style="@style/CustomButtonPrimaryLiveEcg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@id/activity_ecg_test_start_test_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/activity_ecg_test_start_test_button" />

        <TextView
            android:id="@+id/activity_ecg_test_remaining_time_text_view"
            style="@style/SpandanTextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimenPadding8dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/activity_ecg_test_elapsed_time_progress_bar" />

        <ProgressBar
            android:id="@+id/activity_ecg_test_elapsed_time_progress_bar"
            style="@style/Widget.AppCompat.ProgressBar.Horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:indeterminate="false"
            app:layout_constraintBottom_toBottomOf="@id/activity_ecg_test_start_test_button"
            app:layout_constraintEnd_toStartOf="@id/activity_ecg_test_generate_report_button"
            app:layout_constraintStart_toEndOf="@id/activity_ecg_test_retake_test_button"
            app:layout_constraintTop_toTopOf="@id/activity_ecg_test_start_test_button" />


        <TextView
            android:id="@+id/activity_ecg_test_retake_test_button"
            style="@style/CustomButtonPrimaryLiveEcg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/retake_ecg_test"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/activity_ecg_test_start_test_button" />


    </androidx.constraintlayout.widget.ConstraintLayout>

         

